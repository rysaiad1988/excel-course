{
    "parser":"babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
         "jest/globals": true
    },
    rules: {
        object-curly-spacing: off,
        semi: off,
        linebreak-style: off,
        require-jsdoc: off,
        padded-blocks: off,
        eol-last: off,
        indent: off,
        spaced-comment: off,
        max-len: off,
        no-unused-vars: off,
        arrow-parens: off,
        operator-linebreak: off,
        comma-dangle: off,
        brace-style: off,
        block-spacing: off,
        space-before-function-paren: off,
        

    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"],
}